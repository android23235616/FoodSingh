<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foodsingh.mindwires.foodsingh">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE"/>

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/logo7"

        android:label="FoodSingh"
        android:largeHeap="true"
        android:roundIcon="@drawable/logo7"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.Signup_OTP"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.verifying_otp"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.details"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.login_page"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.menu"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.menu_category_wise"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.cart"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.place_order_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.FakeActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.foodsingh.mindwires.foodsingh.order_history"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity android:name="com.foodsingh.mindwires.foodsingh.Splash" android:label=""
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.foodsingh.mindwires.foodsingh.CheckoutActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.foodsingh.mindwires.foodsingh.OrderPlacedActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.foodsingh.mindwires.foodsingh.set_password"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.foodsingh.mindwires.foodsingh.web_view"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:label=""
            android:screenOrientation="portrait"
            android:name="com.foodsingh.mindwires.foodsingh.menu_item_details"
            android:theme="@style/AppTheme"  />
        <activity
            android:screenOrientation="portrait"
            android:name="com.foodsingh.mindwires.foodsingh.Tracker"
            android:label="" />

        <service android:name="com.foodsingh.mindwires.foodsingh.FirebaseMessagingService"

            android:enabled="true"
            android:stopWithTask="false"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service android:name="com.foodsingh.mindwires.foodsingh.fcm" android:label=""
            android:exported="true"
            android:stopWithTask="false"
            android:enabled="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <activity android:name="com.foodsingh.mindwires.foodsingh.NotificationActivity"></activity>
        <activity android:name="com.foodsingh.mindwires.foodsingh.about_us"></activity>
        <activity android:name="com.foodsingh.mindwires.foodsingh.Support"></activity>
    </application>

</manifest>